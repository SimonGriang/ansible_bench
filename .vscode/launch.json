    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/ansible_bench_code/ansible_generator.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src/ansible_bench_code",
            "args": [                
                "-m", "llama3.2",
                "-e", "llamafile", "benchmark",
                "-d", "example",
                "-tt", "exact",
                "-p", "prompts/llamafile_codestral_english_exact/example"
            ]
        }
    ]
}
